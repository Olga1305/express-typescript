{
  "name": "express-typescript",
  "version": "1.0.0",
  "description": "API test for IMMFLY",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev src/app.ts",
    "lint": "eslint --ext .js,.ts --fix src/",
    "start": "node build/app.js",
    "test": "cross-env DEBUG=et:* jest --testMatch \"<rootDir>/test/tests/*.test.js\" --noStackTrace --runInBand"
  },
  "keywords": [
    "node",
    "express",
    "typescript"
  ],
  "author": "Olga Dokukova",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/Olga1305/express-typescript"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "dependencies": {
    "axios": "0.27.2",
    "cross-env": "^7.0.3",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-validator": "6.14.1",
    "http-status-codes": "2.2.0"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "chai": "4.3.6",
    "debug": "4.3.4",
    "dirty-chai": "2.0.1",
    "eslint": "8.17.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.1.0",
    "prettier": "2.6.2",
    "supertest": "6.2.3",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.3"
  }
}